{
  "name": "rm-calculator",
  "version": "0.1.0",
  "homepage": "https://tanguyleloch.github.io/rm-calculator",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /rm-calculator/",
    "build:prod": "ng build --configuration production --base-href /rm-calculator/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "predeploy": "npm run build:prod",
    "deploy": "npx gh-pages -d dist/rm-calculator/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@primeng/themes": "^21.0.2",
    "@primeuix/themes": "^1.2.5",
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.2",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  }
}
